  <row>
    <panel id="panel10">
      <title>Cluster Events Splunkd.log</title>
      <input id="linkCollapse10" type="link" token="tokLinkCollapse10" searchWhenChanged="true" depends="$showCollapseLink10$">
        <label></label>
        <choice value="collapse">-</choice>
        <change>
          <condition value="collapse">

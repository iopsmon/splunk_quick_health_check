  <row>
    <panel id="panel11">
      <title>data hygiene checks</title>
      <input id="linkCollapse11" type="link" token="tokLinkCollapse11" searchWhenChanged="true" depends="$showCollapseLink11$">
        <label></label>
        <choice value="collapse">-</choice>
        <change>
          <condition value="collapse">
            <unset token="showCollapseLink11"></unset>
            <set token="showExpandLink11">true</set>
            <unset token="form.tokLinkCollapse11"></unset>
          </condition>
        </change>
      </input>
      <input id="linkExpand11" type="link" token="tokLinkExpand11" searchWhenChanged="true" depends="$showExpandLink11$">
        <label></label>
        <choice value="expand">+</choice>
        <change>
          <condition value="expand">
            <set token="showCollapseLink11">true</set>
            <unset token="showExpandLink11"></unset>
            <unset token="form.tokLinkExpand11"></unset>
          </condition>
        </change>
      </input>
      <table rejects="$showExpandLink11$">
        <search>
          <query>earliest=-24h index=_internal sourcetype=splunkd component=LineBreakingProcessor OR (component=DateParserVerbose NOT FileClassifier) OR component=AggregatorMiningProcessor 
| rex "Context:\s+source[:=]{1,2}(?&lt;data_source&gt;[^\|]+)\|host[:=]{1,2}(?&lt;data_host&gt;[^\|]+)\|(?&lt;data_sourcetype&gt;[^\|]+)" 
| where NOT match(data_sourcetype, "^splunk_") 
| stats count AS warn_count count(eval(component=="AggregatorMiningProcessor")) AS aggregation_issues count(eval(component=="LineBreakingProcessor")) AS line_breaking_issues count(eval(component=="DateParserVerbose")) AS date_parsing_issues dc(data_host) AS data_hosts dc(data_source) AS data_sources by data_sourcetype 
| rename data_sourcetype AS orig_sourcetype 
| eval severity_level=2 
| sort -warn_count 
| appendpipe 
    [ stats count 
    | where count=0 
    | fields - count 
    | eval severity_level=-1, signature="This check did not produce results"] 
| eval _time=if(isnull(_time), now(), _time) 
| table * 
| fields - _timediff _tc _key punct</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">5</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
